{
    "window.zoomLevel": -0.5,
    "editor.fontSize": 17,
    "terminal.integrated.fontSize": 17,
    "explorer.confirmDelete": false,
    {
        "favorites.sortDirection": "DESC",
        "files.exclude": {
            "appuser-frontend": true,
            "bkmdoc-frontend": true,
            "common-libs": true,
            "corelib": true,
            "laradock": true,
        }
    },
    "better-phpunit.docker.enable": true,
    "better-phpunit.docker.command": "docker exec -ti php56",
    "better-phpunit.docker.paths": {
        "/datos/web/bkmdocapi": "/datos/web/bkmdocapi"
    },
    "better-phpunit.commandSuffix": "-c /datos/web/bkmdocapi/phpunit.xml",
    "workbench.colorTheme": "Winter is Coming (Dark)",
    "workbench.iconTheme": "material-icon-theme",
    //"php.executablePath": "/usr/bin/docker exec -ti php56 /usr/bin/php",
    "php.executablePath": "/datos/web/php7wrapper.sh",
    "php.suggest.basic": false,
    "cSpell.language": "en,es",
    "cSpell.userWords": [
        "CETELEM",
        "Cancelar",
        "Credi",
        "Facturable",
        "carácteres",
        "cotitular",
        "logearse",
        "razón",
        "redirección",
        "reprogramar",
        "sincronización",
        "tipología"
    ],
    "todohighlight.keywords": [
        "DEBUG:",
        "REVIEW:",
        {
            "text": "NOTE:",
            "color": "#fff",
            "backgroundColor": "pink",
            "overviewRulerColor": "pink"
        },
        {
            "text": "HACK:",
            "color": "#000",
            "isWholeLine": false,
        },
    ],
    "todo-tree.excludeGlobs": [
        "**/vendor/**",
        "**/node/node_modules/**",
        "**/node_modules/**",
        "**/dist/**",
        "**/resources/**",
        "**/public/**",
        "**/core/**",
        "**/common-libs/**",
        "**/openlead/**",
        "**/appuser-frontend/**",
        "**/bkmdoc-frontend/**",
    ],
    "todo-tree.customHighlight": {
        "NOTE": {
            "icon": "check",
            "type": "line"
        },
        "FIXME": {
            "foreground": "black",
            "iconColour": "yellow"
        }
    },
    //"artisan.docker.enabled": true,
    //"artisan.docker.command": "docker exec -to workdir php56"
}
